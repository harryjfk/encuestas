
@model Entity.Factor

@{
    ViewBag.Title = "Detalles del Factor";
}
<div class="title-container-details">
    <h3 class="screen-title">Detalles del Factor</h3>

    <a href="/Factor/Index" class="btn btn-primary pull-right"><i class="glyphicon glyphicon-backward"></i> Regresar</a>

    <div class="info-child-screen-details">
	 <div>
            <span class="bold">Tipo: </span> <span>@Model.TipoFactor</span>
        </div>   
		 <div>
            <span class="bold">Nombre: </span> <span>@Model.Nombre</span>
        </div>
		 <div>
            <span class="bold">Activado: </span> <span>@Model.Activado</span>
        </div>  

	 </div>
</div>	
