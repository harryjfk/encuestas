@model Entity.Reportes.SegAuxModel
<style>
    .table {
        white-space: nowrap;
    }

    th {
        text-align: center;
    }
</style>
<div class="table-responsive">
    <table id="tbl-ivf" class="table table-striped table-hover table-bordered table-condensed tabla small">
        <thead>
            <tr class="cabecera">
                <th rowspan="3">Analista</th>
                <th colspan="@(Model.Fechas.Count * 4)">Cantidad</th>
                <th colspan="@(Model.Fechas.Count * 4)">% según analista</th>
                <th colspan="@(Model.Fechas.Count * 4)">% Según estado de encuestas por analista</th>
            </tr>
            <tr class="cabecera">
                <th colspan="@Model.Fechas.Count">Enviados</th>
                <th colspan="@Model.Fechas.Count">No-Enviado</th>
                <th colspan="@Model.Fechas.Count">Consolidados</th>
                <th colspan="@Model.Fechas.Count">Total</th>
                <th colspan="@Model.Fechas.Count">Enviados</th>
                <th colspan="@Model.Fechas.Count">No-Enviado</th>
                <th colspan="@Model.Fechas.Count">Consolidados</th>
                <th colspan="@Model.Fechas.Count">Total</th>
                <th colspan="@Model.Fechas.Count">Enviados</th>
                <th colspan="@Model.Fechas.Count">No-Enviado</th>
                <th colspan="@Model.Fechas.Count">Consolidados</th>
                <th colspan="@Model.Fechas.Count">Total</th>
            </tr>
            <tr class="cabecera">
                @foreach(DateTime dt in Model.Fechas){
                    <th>@dt.Day - @dt.ToString("MMMM").Substring(0, 3)</th>
                }
                @foreach (DateTime dt in Model.Fechas)
                {
                    <th>@dt.Day - @dt.ToString("MMMM").Substring(0, 3)</th>
                }
                @foreach (DateTime dt in Model.Fechas)
                {
                    <th>@dt.Day - @dt.ToString("MMMM").Substring(0, 3)</th>
                }
                @foreach (DateTime dt in Model.Fechas)
                {
                    <th>@dt.Day - @dt.ToString("MMMM").Substring(0, 3)</th>
                }

                @foreach (DateTime dt in Model.Fechas)
                {
                    <th>@dt.Day - @dt.ToString("MMMM").Substring(0, 3)</th>
                }
                @foreach (DateTime dt in Model.Fechas)
                {
                    <th>@dt.Day - @dt.ToString("MMMM").Substring(0, 3)</th>
                }
                @foreach (DateTime dt in Model.Fechas)
                {
                    <th>@dt.Day - @dt.ToString("MMMM").Substring(0, 3)</th>
                }
                @foreach (DateTime dt in Model.Fechas)
                {
                    <th>@dt.Day - @dt.ToString("MMMM").Substring(0, 3)</th>
                }

                @foreach (DateTime dt in Model.Fechas)
                {
                    <th>@dt.Day - @dt.ToString("MMMM").Substring(0, 3)</th>
                }
                @foreach (DateTime dt in Model.Fechas)
                {
                    <th>@dt.Day - @dt.ToString("MMMM").Substring(0, 3)</th>
                }
                @foreach (DateTime dt in Model.Fechas)
                {
                    <th>@dt.Day - @dt.ToString("MMMM").Substring(0, 3)</th>
                }
                @foreach (DateTime dt in Model.Fechas)
                {
                    <th>@dt.Day - @dt.ToString("MMMM").Substring(0, 3)</th>
                }
            </tr>
        </thead>
        <tbody>
            @foreach (var seg in Model.Seguimiento)
            {
                <tr>
                    <td>@seg.Analista</td>

                    @foreach (var item in seg.Cantidad_Enviados)
                    {
                        <td>@item</td>
                    }
                    @foreach (var item in seg.Cantidad_NoEnviados)
                    {
                        <td>@item</td>
                    }
                    @foreach (var item in seg.Cantidad_Consolidados)
                    {
                        <td>@item</td>
                    }
                    @foreach (var item in seg.Cantidad_Total)
                    {
                        <td>@item</td>
                    }

                    @foreach (var item in seg.PorcSegunAnalista_Enviados)
                    {
                        <td>@item</td>
                    }
                    @foreach (var item in seg.PorcSegunAnalista_NoEnviados)
                    {
                        <td>@item</td>
                    }
                    @foreach (var item in seg.PorcSegunAnalista_Consolidados)
                    {
                        <td>@item</td>
                    }
                    @foreach (var item in seg.PorcSegunAnalista_Total)
                    {
                        <td>@item</td>
                    }

                    @foreach (var item in seg.PorcSegunEstado_Enviados)
                    {
                        <td>@item</td>
                    }
                    @foreach (var item in seg.PorcSegunEstado_NoEnviados)
                    {
                        <td>@item</td>
                    }
                    @foreach (var item in seg.PorcSegunEstado_Consolidados)
                    {
                        <td>@item</td>
                    }
                    @foreach (var item in seg.PorcSegunEstado_Total)
                    {
                        <td>@item</td>
                    }
                </tr>
            }
            <tr>
                <td>Totales</td>

                @foreach (var total in Model.Totales.Cantidad_Enviados_Totales)
                {
                    <td>@total</td>
                }
                @foreach (var total in Model.Totales.Cantidad_NoEnviados_Totales)
                {
                    <td>@total</td>
                }
                @foreach (var total in Model.Totales.Cantidad_Consolidados_Totales)
                {
                    <td>@total</td>
                }
                @foreach (var total in Model.Totales.Cantidad_Enviados_Totales)
                {
                    <td>@total</td>
                }

                @foreach (var total in Model.Totales.PorcSegunAnalista_Enviados_Totales)
                {
                    <td>@total</td>
                }
                @foreach (var total in Model.Totales.PorcSegunAnalista_NoEnviados_Totales)
                {
                    <td>@total</td>
                }
                @foreach (var total in Model.Totales.PorcSegunAnalista_Consolidados_Totales)
                {
                    <td>@total</td>
                }
                @foreach (var total in Model.Totales.PorcSegunAnalista_Total_Totales)
                {
                    <td>@total</td>
                }

                @foreach (var total in Model.Totales.PorcSegunEstado_Enviados_Totales)
                {
                    <td>@total</td>
                }
                @foreach (var total in Model.Totales.PorcSegunEstado_NoEnviados_Totales)
                {
                    <td>@total</td>
                }
                @foreach (var total in Model.Totales.PorcSegunEstado_Consolidados_Totales)
                {
                    <td>@total</td>
                }
                @foreach (var total in Model.Totales.PorcSegunEstado_Total_Totales)
                {
                    <td>@total</td>
                }
            </tr>

        </tbody>
    </table>
</div>
